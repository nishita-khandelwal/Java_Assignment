package com.zensar;

public class LinearQueue<G> 
{

		private G [] arr;
		private int size, front, rear;
		
		
		public LinearQueue() {
			super();
		}


		public LinearQueue(G[] arr, int size, int front, int rear) {
			super();
			this.arr = arr;
			this.size = size;
			this.front = front;
			this.rear = rear;
		}
		
		//constructors
		
		//methods for insert and remove operations
	

}
