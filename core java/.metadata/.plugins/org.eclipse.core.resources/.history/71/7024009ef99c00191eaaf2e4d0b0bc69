//stack example
package dayfive;
class Stack
{
   private int arr[];
   private int top, size;
   
   public Stack() 
   {
	   super();
	   size=5;
	   arr=new int[size];
	   top=-1;
   }
   public void push(int data) 
   {
	   if(isFull())
	   {
		   System.out.println("Stack full");
	   }
	   else 
	   {
		   arr[top++]=data;  
	   }
	   
   }
   public int pop() 
   {
	   if(isEmpty())
	   {
		   System.out.println("Stack Empty");
		   return 0;
	   }
	   else {
		   return arr[top--];
	}
	   
   }
   public boolean isFull() 
   {
	   return top==size-1;
   }
   public boolean isEmpty() 
   {
	   return top==-1;
   }
   
}

public class StackExample 
{
	public static void main(String[] args) 
	{
	   Stack stack=new Stack();	
	   stack.push(34);
	   stack.push(12);
	   stack.push(364);
	   stack.push(123);
	   stack.push(34);
	   stack.push(12334);
	   System.out.println(stack.pop());
	   System.out.println(stack.pop());
	   System.out.println(stack.pop());
	   
	}
}
