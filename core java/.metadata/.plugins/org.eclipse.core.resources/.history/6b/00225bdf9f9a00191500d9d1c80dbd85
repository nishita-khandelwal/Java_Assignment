package com.zensar.polydemo;

public class Employee {
	private int empid;
	private String name;
	private Date dob;
	
	public Employee() {
		empid=1;
		name="abc";
		dob=new Date();
	}

	public Employee(int empid, String name, int d, int m, int y) {
		super();
		this.empid = empid;
		this.name = name;
		dob =Date(d,m,y);
	}
	

}
